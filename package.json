{
  "name": "@aceandtate/breakpoints",
  "version": "0.0.7",
  "description": "Ace and Tate's Breakpoints",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "pika build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run -s build"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "files": ["*.js", "build", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aceandtate/breakpoints.git"
  },
  "keywords": ["breakpoints"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aceandtate/breakpoints/issues"
  },
  "homepage": "https://github.com/aceandtate/breakpoints#readme",
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "prettier": "1.10.2"
  },
  "dependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.0",
    "@pika/plugin-build-types": "^0.6.0",
    "@pika/plugin-build-web": "^0.6.0",
    "@pika/plugin-standard-pkg": "^0.6.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": ["**/*.test.*"]
        }
      ],
      ["@pika/plugin-build-node"],
      ["@pika/plugin-build-web"],
      ["@pika/plugin-build-types"]
    ]
  }
}
